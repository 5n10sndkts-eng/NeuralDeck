{
  "name": "neuraldeck",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:p0": "jest --testNamePattern='\\[P0\\]'",
    "test:p1": "jest --testNamePattern='\\[P0\\]|\\[P1\\]'",
    "test:voice": "jest voice-commands.test",
    "test:vision": "jest vision-pipeline.test",
    "test:audio": "jest audio-cpu-usage.test",
    "test:epic3": "jest '(voice|vision|audio).*test'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:perf": "playwright test --grep '@perf'",
    "test:e2e:security": "playwright test --grep '@security'",
    "test:e2e:autonomy": "playwright test --grep '@autonomy'",
    "test:perf:k6": "k6 run tests/nfr/performance/api-load-test.k6.js",
    "test:burn-in": "bash scripts/burn-in-changed.sh",
    "test:burn-in:strict": "bash scripts/burn-in-changed.sh 20",
    "test:ci": "bash scripts/ci-local.sh",
    "test:integration": "jest --testPathPattern='integration' --testPathIgnorePatterns=''"
  },
  "dependencies": {
    "@fastify/compress": "^8.3.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@langchain/community": "^1.1.1",
    "@langchain/core": "^1.1.8",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "@types/dagre": "^0.7.53",
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "dexie": "^4.2.1",
    "express": "^5.1.0",
    "fastify": "^5.6.2",
    "framer-motion": "^12.23.24",
    "fs": "^0.0.1-security",
    "langchain": "^1.2.3",
    "lucide-react": "^0.554.0",
    "path": "^0.12.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "^11.11.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.182.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.0",
    "@types/puppeteer": "^5.4.7",
    "@types/testing-library__react": "^10.0.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "fake-indexeddb": "^6.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.34.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
